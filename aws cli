create 


aws iam create-user --user-name mike

aws iam create-login-profile --user-name luke --password luke@123

aws iam create-login-profile --user-name luke --password luke@123 --password-reset-required



delete 

if login exist

aws iam delete-login-profile --user-name luke

aws iam delete-user --user-name luke



list

aws iam list-users
